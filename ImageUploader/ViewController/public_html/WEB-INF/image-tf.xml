<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="image-tf">
    <default-activity>image</default-activity>
    <managed-bean id="__1">
      <managed-bean-name>ImageRepoBean</managed-bean-name>
      <managed-bean-class>view.ImageRepoBean</managed-bean-class>
      <managed-bean-scope>backingBean</managed-bean-scope>
    </managed-bean>
    <view id="image">
      <page>/image.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
